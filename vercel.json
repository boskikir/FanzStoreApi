{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/iyah.json",
      "dest": "/index.js"
    },
    {
      "src": "/stats.json",
      "dest": "/index.js"
    },
    {
      "src": "/(.*\\.(html|css|js|json|jpg|png|ico|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}

