{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/iyah.json", "dest": "/public/iyah.json" },
    { "src": "/stats.json", "dest": "/public/stats.json" },
    { "src": "/user/(.*)", "dest": "/public/user/$1" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
